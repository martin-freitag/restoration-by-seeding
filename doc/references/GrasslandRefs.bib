  file = {C\:\\Users\\m_frei22\\Zotero\\storage\\MNKUIC39\\Bergmann et al. - 2020 - The fungal collaboration gradient dominates the ro.pdf;C\:\\Users\\m_frei22\\Zotero\\storage\\LBHCJKPG\\eaba3756.html},
