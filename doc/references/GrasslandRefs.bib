@article{zemmrich2010,
  title = {Plant Communities along an Elevation Gradient under Special Consideration of Grazing in {{Western Mongolia}}},
  author = {Zemmrich, Anne and Hilbig, Werner and Oyuunchimeg, Damdins{\"u}rengiyn},
  year = {2010},
