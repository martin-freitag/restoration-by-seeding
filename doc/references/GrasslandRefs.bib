@article{smith2003,
  title = {Soil Microbial Community, Fertility, Vegetation and Diversity as Targets in the Restoration Management of a Meadow Grassland},
  author = {Smith, R. S. and Shiel, R. S. and Bardgett, R. D. and Millward, D. and Corkhill, P. and Rolph, G. and Hobbs, P. J. and Peacock, S.},
  year = {2003},
  volume = {40},
