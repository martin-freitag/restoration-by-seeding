@article{kelemen2013,
  title = {Mechanisms Shaping Plant Biomass and Species Richness: Plant Strategies and Litter Effect in Alkali and Loess Grasslands},
  author = {Kelemen, Andr{\'a}s and T{\"o}r{\"o}k, P{\'e}ter and Valk{\'o}, Orsolya and Migl{\'e}cz, Tam{\'a}s and T{\'o}thm{\'e}r{\'e}sz, B{\'e}la and Scheiner, Sam},
  year = {2013},
  month = jan,
